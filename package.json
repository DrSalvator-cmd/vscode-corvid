{
  "name": "vscode-corvid",
  "displayName": "Corvid",
  "description": "The extension to work with corvid-cli.",
  "version": "0.3.1",
  "author": "alexanderz@wix.com",
  "publisher": "shoonia",
  "scripts": {
    "eslint": "eslint . --fix"
  },
  "main": "./extension.js",
  "icon": "icons/logo.png",
  "activationEvents": [
    "workspaceContains:.corvid/corvidrc.json",
    "workspaceContains:**/*.jsw"
  ],
  "contributes": {
    "commands": [],
    "languages": [
      {
        "id": "javascript",
        "extensions": [
          ".jsw",
          ".js"
        ]
      },
      {
        "id": "jsonc",
        "extensions": [
          ".wix",
          "jobs.config"
        ]
      }
    ],
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets.json"
      }
    ]
  },
  "categories": [
    "Other"
  ],
  "engines": {
    "vscode": "^1.37.0"
  },
  "devDependencies": {
    "eslint": "^6.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shoonia/vscode-corvid.git"
  },
  "keywords": [
    "wix",
    "corvid",
    "wixcode"
  ],
  "license": "MIT"
}
